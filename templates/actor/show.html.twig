{% extends 'base.html.twig' %}

{% block body %}
        <div>
            <h2>{{ actor.name }}</h2>
            <ul>
                {% for program in actor.programs %}
                    <li>
                        <a href="{{ path('program_show', {slug :program.slug} ) }}">{{ program.title }}</a>
                    </li>
                {% endfor %}
            </ul>
        </div>
{% endblock %}